{
  "name": "chrome-timer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "test",
  "homepage_url": "https://gman.io",
  "browser_action": {
    "default_icon": "dist/favicon.ico",
    "default_popup": "dist/index.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
